{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "C:\\MyAppLogs\\WorkflowLib.DataStorage.InMemoryService.Grpc\\log.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  
  "AppSettings": {
    "EnvironmentVariable": ""
  }
}
